{
  "name": "TabSwitcher",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "Kamran Ahmed",
  "description": "Switch between the tabs with ease",
  "homepage_url": "https://github.com/kamranahmedse/tab-switcher",
  "icons": {
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
